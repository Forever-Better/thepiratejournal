{
  "name": "thepiratejournal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch-codegen": "graphql-codegen --watch",
    "dev": "next dev",
    "inspect": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "yarn build-codegen && yarn build-nextjs",
    "build-nextjs": "next build",
    "build-codegen": "graphql-codegen",
    "build:css": "postcss ./src/styles/styles.scss -o ./src/styles/index.scss",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx",
    "format": "prettier --write **/*.{ts,tsx,scss,mdx}",
    "update-schema": "graphql-codegen -c ./codegen.download.yaml",
    "prepare": "cd .. && husky install client/.husky",
    "test": "jest",
    "typecheck": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint --fix"
    ]
  },
